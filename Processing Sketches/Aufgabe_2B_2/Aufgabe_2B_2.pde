void setup() {
  //Setzt die Fenstergroesse auf 600x600 Pixel.
  size(600, 600);
  
    //setzt die Hintergrundfarbe auf gruen
    background(0, 255, 0);

  //legt die Farbe der Kreise auf Hotpink fest
  fill(255, 105, 180);

  //kreiert 5 Kreise, welche in der Box platziert werden
  circle(300, 300, 100);
  circle(50, 50, 100);
  circle(550, 50, 100);
  circle(50, 550, 100);
  circle(550, 550, 100);
}
