void setup() {

  //setzt die Fenstergroesse auf 800x600 Pixel
  size(800, 600);
  
  //Stellt die Hintergrundsfarbe ein
  background(0);
  
  //legt die Kreisfarbe fest
  fill(255);
  
  //erstellt die Kreise
  circle(700, 300, 150);
  circle(550, 300, 150);
  circle(400, 300, 150);
  circle(250, 300, 150);
  circle(100, 300, 150);

}
